{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "iam:*"
      ],
      "Resource": [
        "arn:aws:iam::123456789012:user/*"
      ]
    },
    {
      "Effect": "Deny",
      "Action": [
        "iam:*"
      ],
      "NotResource": [
        "arn:aws:iam::123456789012:user/something@domain.com"
      ]
    }

  ]
}
